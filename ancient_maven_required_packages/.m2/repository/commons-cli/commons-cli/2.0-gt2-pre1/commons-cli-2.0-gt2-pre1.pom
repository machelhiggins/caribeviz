      </roles>
    </developer>
    <developer>
      <id>jkeyes</id>
      <name>John Keyes</name>
      <email>jbjk@mac.com</email>
      <organization>integral Source</organization>
      <roles>
        <role>contributed ideas and code from Optz</role>
      </roles>
    </developer>
    <developer>
      <id>roxspring</id>
      <name>Rob Oxspring</name>
      <email>roxspring@imapmail.org</email>
